{
  "name": "vechain-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "mocha",
    "node:start": "node src/main.js",
    "node:stop": "pkill -f 'node src/main.js'",
    "api:start": "node src/server.js",
    "api:stop": "pkill -f 'node src/server.js'",
    "ui:start": "react-scripts start",
    "ui:build": "react-scripts build",
    "ui:test": "react-scripts test",
    "ui:eject": "react-scripts eject",
    "start": "concurrently --kill-others-on-fail 'npm run node:start' 'npm run api:start' 'npm run ui:start'",
    "build": "npm run ui:build",
    "test": "npm run ui:test"
  },
  "dependencies": {
    "express": "^4.17.1",
    "vechain-node": "^1.0.0",
    "winston": "^4.4.0"
  },
  "devDependencies": {
    "chai": "^5.0.1",
    "concurrently": "^5.3.0",
    "react-scripts": "4.0.0",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": "12.x"
  },
  "types": "ui/src/@types/react/index.d.ts"
}
